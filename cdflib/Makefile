# Rudimentary Makefile written by Jo Valentine-Cooper
# Based on Makefile steps generated by Math::CDF's Makefile.PL for this code,
# with some spurious includes and defines and other Perly requirements removed.

COMPILER=cc
CFLAGS=-c -pipe -fwrapv -fno-strict-aliasing -fstack-protector -O2 -fPIC

libcdflib.a : dcdflib.o ipmpar.o
	ar cr libcdflib.a dcdflib.o ipmpar.o

.PHONY: clean
clean :
	rm -f dcdflib.o ipmpar.o libcdflib.a

dcdflib.o :
	${COMPILER} ${CFLAGS} dcdflib.c

ipmpar.o :
	${COMPILER} ${CFLAGS} ipmpar.c

