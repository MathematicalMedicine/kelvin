	"TM" -> "SS" ;
	"TP" -> "DTTP" ;
	"TP" -> "QTTP" ;
	"TP" -> "CTTP" ;
	"SA" -> "DTMP" ;
	"SA" -> "QTMP" ;
	"SA" -> "CTMP" ;
	"SS" -> "DTMP" ;
	"SS" -> "QTMP" ;
	"SS" -> "CTMP" ;
	"DTTP" -> "LE" ;
	"DTTP" -> "LD" ;
	"CTTP" -> "CSTP" ;
	"CTTP" -> "TDTP" ;
	"QTTP" -> "CSTP" ;
	"QTTP" -> "TDTP" ;
	"QTMP" -> "CS" ;
	"QTMP" -> "TD" ;
	"CTMP" -> "CS" ;
	"CTMP" -> "TD" ;
	"CSTP" -> "LE" ;
	"CSTP" -> "LD" ;
	"TDTP" -> "LE" ;
	"TDTP" -> "LD" ;

