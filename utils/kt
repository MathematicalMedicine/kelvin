where=`dirname "$1"`
builtin cd "$where"
echo -n $where...
here=`pwd`
a=`expr "$here" : '.*dkelvin.*'`
if [ $a -eq 0 ]
then
  time $HOME/kelvin/trunk/kelvin kelvin.conf >&kelvin.out
else
  time $HOME/kelvin/trunk/dkelvin kelvin.conf >&dkelvin.out
fi
if diff avghet.out avghet.out-baseline
then
  echo -n "avghet.out=baseline "
else
  echo -n "ERROR! - avghet.out!=baseline "
fi
if [ "-e" "ppl.out" ];
then
  if diff ppl.out ppl.out-baseline
  then
    echo -n "ppl.out=baseline"
  else
    echo -n ERROR! - ppl.out!=baseline
  fi
fi
echo ""
builtin cd "$OLDPWD"
